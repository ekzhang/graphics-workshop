  "scripts": {
    "assets": "npm-run-all clean main:build --serial assets:dist assets:version assets:zip",
    "assets:dist": "mkdir -p dist && cp docs/assets/tweakpane*.js dist",
    "assets:version": "node scripts/assets-version.js",
    "assets:zip": "zip -x '*types*' -j -r tweakpane-$(cat package.json | npx json version).zip dist",
    "build": "run-s main:build doc:build",
    "clean": "rm -rf dist docs test tweakpane-*.tgz tweakpane-*.zip test-module/node_modules",
    "coverage": "run-s coverage:report coverage:coveralls",
    "coverage:coveralls": "mkdir -p test/coverage && nyc report --reporter=text-lcov > test/coverage/lcov.info",
    "coverage:report": "run-s coverage:report:xml coverage:report:html",
    "coverage:report:html": "nyc report --reporter=html --reporter=text --reporter=lcovonly --report-dir=test/coverage",
    "coverage:report:xml": "TS_NODE_PROJECT='lib/tsconfig.json' nyc --reporter=none --extension '.ts' --exclude '**/*-test.ts' mocha --require ts-node/register --reporter mocha-junit-reporter --reporter-options mochaFile=./test/results/results.xml 'lib/**/*-test.ts'",
    "doc:build": "run-p doc:build:*",
    "doc:build:html": "node scripts/doc-build-html.js",
    "doc:build:img": "mkdir -p docs/assets && cp -r src/doc/img/* docs/assets",
    "doc:build:refs": "typedoc --options src/doc/typedoc.json lib",
    "doc:build:scss": "mkdir -p docs/assets && node-sass src/doc/sass/bundle.scss | postcss --config postcss.config.js > docs/assets/bundle.css",
    "doc:build:ts": "rollup --config rollup-doc.config.js",
    "doc:format": "run-p doc:format:*",
    "doc:format:scss": "prettier --parser scss --write 'src/doc/sass/**/*.scss'",
    "doc:format:ts": "eslint --ext .ts --fix 'src/doc/ts/**/*.ts'",
    "doc:server": "http-server ./docs",
    "doc:test": "run-p doc:test:*",
    "doc:test:scss": "prettier --parser scss --list-different 'src/doc/sass/**/*.scss'",
    "doc:test:ts": "eslint --ext .ts 'src/doc/ts/**/*.ts'",
    "doc:watch": "run-p doc:watch:*",
    "doc:watch:html": "onchange 'src/doc/template/**/*.html' -- npm run doc:build:html",
    "doc:watch:scss": "onchange 'src/doc/sass/**/*.scss' -- npm run doc:build:scss",
    "doc:watch:ts": "onchange 'src/doc/ts/**/*.ts' -- npm run doc:build:ts",
    "format": "run-p main:format doc:format",
    "main:build": "run-p main:build:*",
    "main:build:dev": "rollup --config rollup.config.js",
    "main:build:dts": "tsc --project lib/tsconfig-dts.json",
    "main:build:prod": "rollup --config rollup.config.js --environment BUILD:production",
    "main:format": "run-p main:format:*",
    "main:format:scss": "prettier --parser scss --write 'lib/sass/**/*.scss'",
    "main:format:ts": "eslint --ext .ts --fix 'lib/**/*.ts'",
    "main:prepare": "npm-run-all main:build --serial assets:dist",
    "main:test": "run-s main:test:scss main:test:ts",
    "main:test:scss": "prettier --parser scss --list-different 'lib/sass/**/*.scss'",
    "main:test:ts": "run-s main:test:ts:static main:test:ts:dynamic main:test:ts:module",
    "main:test:ts:dynamic": "TS_NODE_PROJECT='lib/tsconfig.json' nyc --extension '.ts' --exclude '**/*-test.ts' mocha --require ts-node/register 'lib/**/*-test.ts'",
    "main:test:ts:dynamic:inspect": "TS_NODE_PROJECT='lib/tsconfig.json' nyc --extension '.ts' --exclude '**/*-test.ts' mocha --require ts-node/register 'lib/**/*-test.ts' --inspect-brk",
    "main:test:ts:module": "npm pack && node scripts/main-test-ts-module-pre.js && (cd test-module && rm -rf ./node_modules/tweakpane && npm install && npm test)",
    "main:test:ts:static": "eslint --ext .ts 'lib/**/*.ts'",
    "main:watch": "run-p main:watch:*",
    "main:watch:scss": "onchange 'lib/sass/**/*.scss' -- npm run main:build:dev",
    "main:watch:ts": "onchange 'lib/**/*.ts' -- npm run main:build:dev",
    "prepare": "run-s clean main:prepare",
    "prepublishOnly": "npm test",
    "start": "npm-run-all main:build:dev doc:build --parallel main:watch doc:watch doc:server",
    "test": "run-s doc:test main:test"
  },
